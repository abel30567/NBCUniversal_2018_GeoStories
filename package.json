{
  "name": "yories",
  "version": "1.0.0",
  "description": "IG and Snapchat stories with location and tag filtering",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/concurrently \"npm run serve:client\" \"npm run serve:server\"",
    "serve:client": "cd client && npm start",
    "serve:server": "cd server && npm start",
    "update:client": "cd client && npm i",
    "update:server": "cd server && npm i",
    "postinstall": "./node_modules/.bin/concurrently \"npm run update:client\" \"npm run update:server\"",
    "stop": "",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liquidcarrot/yories.git"
  },
  "keywords": [
    "social",
    "sharing",
    "video"
  ],
